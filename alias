#include "simpleshell.h"

/**
 * struct aliases - Struct that use an alias
 * @alias_name: the name of the of the alias
 * @real_name: the real name of the command
 */
typedef struct aliases {
	char *alias_name;
	char *real_name;
} alias;

/**
 * exe_alias - function that execute alias
 * @av: an array of string (command passed by user)
 * Return: builtin funtion or execute av
 */
int exe_alias(char **av)
{
	alias my_aliases[] = {
		{“ls”, “ls — color=auto”},
		{“ll”, “ls -alF”},
		{“la”, “ls -A”},
		{NULL, NULL},
	} my_aliases;
	int i;

	for (i = 0; my_aliases[i].name != NULL; i++)
		if (_strcmp(av[0], my_aliases[i].alias_name) == 0)
		{
		        my_aliases[i].real_name;
			return (0);
		}
	return (-1);
}
